<link rel="stylesheet" href="/css/cources.css">

<section class="main flex-column">
    <div class="header flex-column">
        <h1 class="intro-fade">Specialized Words</h1>
        <p class="matn">کلمات تخصصی</p>
    </div>

    <div class="container">
        <table>
            <thead>
                <tr>
                    <th>عنوان کلمه</th>
                    <th>معنای کلمه</th>
                </tr>
            </thead>
            <% keywords.forEach(keyword=>{ %>
            <tbody>
                    <tr>
                        <td>
                            <%= keyword.name %>
                        </td>
                        <td><%- keyword.meaning %></td>
                    </tr>
                </tbody>
                <% }) %>
        </table>
    </div>
</section>
